import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        sc.close();
        int term = 0;
        int diff = 2;
        for (int i = 0; i < n; i++) {
            System.out.print(term);
            if (i != n - 1) System.out.print(" ");
            term += diff;
            diff += (i % 2 == 0) ? 4 : 0;
        }
    }
}
